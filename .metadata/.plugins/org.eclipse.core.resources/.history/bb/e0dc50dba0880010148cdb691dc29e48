package varAgs;

public class VarAgs {
	
	public void display(int ...n) {
		//here the three dots(...) means array
		
		System.out.println("Length of Array "+ n.length);
		for (int i : n) {
			System.out.println(i);
		}
	}
	/*public void display(int n1,int n2) {
		
