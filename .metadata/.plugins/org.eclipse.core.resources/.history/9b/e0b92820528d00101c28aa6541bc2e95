package BankInheritance;

public class BankMain {

	public static void main(String[] args) {
		SavingAccount sobj=new SavingAccount(101, "Saving", 90000, "SBI23874692",4.9f);
		CurrentAccount cobj=new CurrentAccount(102, "current", 37364876, "ICICI3875", "dgfskj");
		System.out.println("");
	}
	public void display(Account aobj) {
		System.out.println("Account Number :"+aobj.getAccNumber());
		System.out.println("Account Type :"+ aobj.getAccType());
		System.out.println("Account Balence :"+ aobj.getAccBal());
		System.out.println("Account IFSC Code :"+ aobj.getIfscCode());
		
		if(aobj instanceof SavingAccount) {
			SavingAccount sacc=(SavingAccount)aobj;
			System.out.println("Interest Rate :"+ sacc.getInterestRate());
		}
		if(aobj instanceof CurrentAccount) {
			CurrentAccount cacc=(CurrentAccount)aobj;
			System.out.println("Company Name :"+ cacc.getCompanyName());
		}
	}

}
