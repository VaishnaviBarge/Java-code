package StaticDataMembers;

public class Bank {

	int accNumber;			//instance variable
	static int ifsccode;	//class variable
	
	static {
		System.out.println("Static block.....");
	}
	Bank(){
		System.out.println("it's constructor");
	}
	
	public void show() {
		System.out.println("=================================");
		System.out.println("Value of acc number is "+ accNumber);
		System.out.println("Value of ifsc Code  is "+ ifsccode);
	}
	
	public static void display() {
		Bank temp=new Bank();
		System.out.println("Value of acc number is "+ temp.accNumber);
		System.out.println("Value of ifsc Code  is "+ ifsccode);
	}
}
